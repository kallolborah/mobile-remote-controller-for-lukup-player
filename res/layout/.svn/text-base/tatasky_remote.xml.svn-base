<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="fill_parent"
    android:layout_height="fill_parent"
    android:background="@color/pink"
    android:orientation="vertical" >

    <LinearLayout
        android:id="@+id/tatasky_layer1"
        android:layout_width="fill_parent"
        android:layout_height="50dip"
        android:background="@drawable/gradient_menu"
        android:orientation="horizontal" >

        <LinearLayout
            android:id="@+id/dropdown"
            android:layout_width="240dip"
            android:layout_height="fill_parent"
            android:layout_gravity="center"
            android:background="@drawable/list_selector"
            android:orientation="horizontal" >

            <TextView
                android:id="@+id/dropdownText"
                android:layout_width="184dip"
                android:layout_height="fill_parent"
                android:layout_marginLeft="12dip"
                android:gravity="left|center_vertical"
                android:text="Select Service"
                android:textColor="#FFFFFF"
                android:textSize="18dip"
                android:textStyle="bold" />

            <Button
                android:id="@+id/dropdownButton"
                android:layout_width="32dip"
                android:layout_height="32dip"
                android:layout_gravity="center"
                android:background="@drawable/icn_dropdown"
                android:gravity="center" />
        </LinearLayout>

        <ImageButton
            android:id="@+id/tatasky_offBtn"
            android:layout_width="80dip"
            android:layout_height="fill_parent"
            android:background="@drawable/radialgradientselector"
            android:src="@drawable/icn_power"
            android:textColor="#d7148d"
            android:textStyle="bold" />
    </LinearLayout>

    <TableLayout
        android:id="@+id/tatasky_layer2"
        android:layout_width="fill_parent"
        android:layout_height="40dip"
        android:background="@drawable/gradient_menu" >

        <TableRow
            android:layout_width="fill_parent"
            android:layout_height="fill_parent"
            android:background="@android:color/transparent" >

            <ImageButton
                android:id="@+id/tatasky_playBtn"
                android:layout_width="0dip"
                android:layout_height="fill_parent"
                android:layout_weight="1"
                android:background="@drawable/radialgradientselector"
                android:src="@drawable/tatasky_icn_play"
                android:textColor="#d7148d"
                android:textStyle="bold" />

            <ImageButton
                android:id="@+id/tatasky_pauseBtn"
                android:layout_width="0dip"
                android:layout_height="40dip"
                android:layout_marginLeft="1dip"
                android:layout_weight="1"
                android:background="@drawable/radialgradientselector"
                android:src="@drawable/tatasky_icn_pause"
                android:textColor="#d7148d"
                android:textStyle="bold" />

            <ImageButton
                android:id="@+id/tatasky_forwardBtn"
                android:layout_width="0dip"
                android:layout_height="40dip"
                android:layout_marginLeft="1dip"
                android:layout_weight="1"
                android:background="@drawable/radialgradientselector"
                android:src="@drawable/tatasky_icn_fastforward"
                android:textColor="#d7148d"
                android:textStyle="bold" />

            <ImageButton
                android:id="@+id/tatasky_rewindBtn"
                android:layout_width="0dip"
                android:layout_height="40dip"
                android:layout_marginLeft="1dip"
                android:layout_weight="1"
                android:background="@drawable/radialgradientselector"
                android:src="@drawable/tatasky_icn_fastrewind"
                android:textColor="#d7148d" />

            <ImageButton
                android:id="@+id/tatasky_recordBtn"
                android:layout_width="0dip"
                android:layout_height="fill_parent"
                android:layout_marginLeft="1dip"
                android:layout_weight="1"
                android:background="@drawable/radialgradientselector"
                android:src="@drawable/tatasky_icn_record"
                android:textColor="#d7148d"
                android:textStyle="bold" />
        </TableRow>
    </TableLayout>

    <TableLayout
        android:id="@+id/tatasky_layer3"
        android:layout_width="fill_parent"
        android:layout_height="40dip"
        android:background="@drawable/gradient_menu" >

        <TableRow
            android:layout_width="fill_parent"
            android:layout_height="fill_parent"
            android:background="@android:color/transparent" >

            <ImageButton
                android:id="@+id/tatasky_redBtn"
                android:layout_width="0dip"
                android:layout_height="fill_parent"
                android:layout_weight="1"
                android:background="@drawable/radialgradientselector"
                android:src="@drawable/tatasky_red"
                android:textColor="#d7148d"
                android:textStyle="bold" />

            <ImageButton
                android:id="@+id/tatasky_greenBtn"
                android:layout_width="0dip"
                android:layout_height="40dip"
                android:layout_marginLeft="1dip"
                android:layout_weight="1"
                android:background="@drawable/radialgradientselector"
                android:src="@drawable/tatasky__green"
                android:textColor="#d7148d"
                android:textStyle="bold" />

            <ImageButton
                android:id="@+id/tatasky_yellowBtn"
                android:layout_width="0dip"
                android:layout_height="40dip"
                android:layout_marginLeft="1dip"
                android:layout_weight="1"
                android:background="@drawable/radialgradientselector"
                android:src="@drawable/tatasky_yellow"
                android:textColor="#d7148d"
                android:textStyle="bold" />

            <ImageButton
                android:id="@+id/tatasky_blueBtn"
                android:layout_width="0dip"
                android:layout_height="fill_parent"
                android:layout_marginLeft="1dip"
                android:layout_weight="1"
                android:background="@drawable/radialgradientselector"
                android:src="@drawable/tatasky_blue"
                android:textColor="#d7148d"
                android:textStyle="bold" />
        </TableRow>
    </TableLayout>

    <RelativeLayout
        android:id="@+id/tatasky_middlelayer"
        android:layout_width="fill_parent"
        android:layout_height="160dip"
        android:background="@drawable/gradient_menu" >

        <RelativeLayout
            android:id="@+id/tatasky_middlelayer_portion1"
            android:layout_width="80dip"
            android:layout_height="fill_parent"
            android:background="@android:color/transparent" >

            <Button
                android:id="@+id/tatasky_backBtn"
                android:layout_width="fill_parent"
                android:layout_height="40dip"
                android:layout_alignParentBottom="true"
                android:background="@drawable/radialgradientselector"
                android:text="Back"
                android:textColor="#FFFFFF"
                android:textSize="16dip"
                android:textStyle="bold" />
        </RelativeLayout>

        <RelativeLayout
            android:id="@+id/tatasky_middlelayer_portion2"
            android:layout_width="160dip"
            android:layout_height="fill_parent"
            android:layout_toRightOf="@+id/tatasky_middlelayer_portion1"
            android:background="@android:color/transparent" >

            <Button
                android:id="@+id/tatasky_upArrowBtn"
                android:layout_width="114dip"
                android:layout_height="58dip"
                android:layout_alignParentTop="true"
                android:layout_marginLeft="23dip"
                android:background="@drawable/tatasky_nav_up" />

            <Button
                android:id="@+id/tatasky_downArrowBtn"
                android:layout_width="114dip"
                android:layout_height="58dip"
                android:layout_alignParentBottom="true"
                android:layout_marginLeft="23dip"
                android:background="@drawable/tatasky_nav_down" />

            <Button
                android:id="@+id/tatasky_leftArrowBtn"
                android:layout_width="58dip"
                android:layout_height="114dip"
                android:layout_alignParentLeft="true"
                android:layout_marginTop="23dip"
                android:background="@drawable/tatasky_nav_left" />

            <Button
                android:id="@+id/tatasky_rightArrowBtn"
                android:layout_width="58dip"
                android:layout_height="114dip"
                android:layout_alignParentRight="true"
                android:layout_marginTop="23dip"
                android:background="@drawable/tatasky_nav_right" />

            <Button
                android:id="@+id/tatasky_center_select_Btn"
                android:layout_width="64dip"
                android:layout_height="64dip"
                android:layout_centerInParent="true"
                android:background="@drawable/tatasky_nav_select" />
        </RelativeLayout>

        <RelativeLayout
            android:id="@+id/tatasky_middlelayer_portion3"
            android:layout_width="80dip"
            android:layout_height="fill_parent"
            android:layout_toRightOf="@+id/tatasky_middlelayer_portion2"
            android:background="@android:color/transparent" >

            <Button
                android:id="@+id/tatasky_hekpBtn"
                android:layout_width="fill_parent"
                android:layout_height="40dip"
                android:layout_alignParentBottom="true"
                android:background="@drawable/radialgradientselector"
                android:text="Help"
                android:textColor="#FFFFFF"
                android:textSize="16dip"
                android:textStyle="bold" />
        </RelativeLayout>
    </RelativeLayout>

    <TableLayout
        android:id="@+id/tatasky_layer_no_pad_1"
        android:layout_width="fill_parent"
        android:layout_height="40dip"
        android:background="@drawable/gradient_menu" >

        <TableRow
            android:layout_width="fill_parent"
            android:layout_height="fill_parent"
            android:background="@android:color/transparent" >

            <Button
                android:id="@+id/tatasky_oneBtn"
                android:layout_width="0dip"
                android:layout_height="fill_parent"
                android:layout_weight="1"
                android:background="@drawable/radialgradientselector"
                android:text="1.&apos;_"
                android:textColor="#FFFFFF"
                android:textSize="16dip"
                android:textStyle="bold" />

            <Button
                android:id="@+id/tatasky_twoBtn"
                android:layout_width="0dip"
                android:layout_height="fill_parent"
                android:layout_marginLeft="1dip"
                android:layout_weight="1"
                android:background="@drawable/radialgradientselector"
                android:text="2abc"
                android:textColor="#FFFFFF"
                android:textSize="16dip"
                android:textStyle="bold" />

            <Button
                android:id="@+id/tatasky_threeBtn"
                android:layout_width="0dip"
                android:layout_height="fill_parent"
                android:layout_marginLeft="1dip"
                android:layout_weight="1"
                android:background="@drawable/radialgradientselector"
                android:text="3def"
                android:textColor="#FFFFFF"
                android:textSize="16dip"
                android:textStyle="bold" />
        </TableRow>
    </TableLayout>

    <TableLayout
        android:id="@+id/tatasky_layer_no_pad_2"
        android:layout_width="fill_parent"
        android:layout_height="40dip"
        android:background="@drawable/gradient_menu" >

        <TableRow
            android:layout_width="fill_parent"
            android:layout_height="fill_parent"
            android:background="@android:color/transparent" >

            <Button
                android:id="@+id/tatasky_fourBtn"
                android:layout_width="0dip"
                android:layout_height="fill_parent"
                android:layout_weight="1"
                android:background="@drawable/radialgradientselector"
                android:text="4ghi"
                android:textColor="#FFFFFF"
                android:textSize="16dip"
                android:textStyle="bold" />

            <Button
                android:id="@+id/tatasky_fiveBtn"
                android:layout_width="0dip"
                android:layout_height="fill_parent"
                android:layout_marginLeft="1dip"
                android:layout_weight="1"
                android:background="@drawable/radialgradientselector"
                android:text="5jkl"
                android:textColor="#FFFFFF"
                android:textSize="16dip"
                android:textStyle="bold" />

            <Button
                android:id="@+id/tatasky_sixBtn"
                android:layout_width="0dip"
                android:layout_height="fill_parent"
                android:layout_marginLeft="1dip"
                android:layout_weight="1"
                android:background="@drawable/radialgradientselector"
                android:text="6mno"
                android:textColor="#FFFFFF"
                android:textSize="16dip"
                android:textStyle="bold" />
        </TableRow>
    </TableLayout>

    <TableLayout
        android:id="@+id/tatasky_layer_no_pad_3"
        android:layout_width="fill_parent"
        android:layout_height="40dip"
        android:background="@drawable/gradient_menu" >

        <TableRow
            android:layout_width="fill_parent"
            android:layout_height="fill_parent"
            android:background="@android:color/transparent" >

            <Button
                android:id="@+id/tatasky_sevenBtn"
                android:layout_width="0dip"
                android:layout_height="fill_parent"
                android:layout_weight="1"
                android:background="@drawable/radialgradientselector"
                android:text="7pqrs"
                android:textColor="#FFFFFF"
                android:textSize="16dip"
                android:textStyle="bold" />

            <Button
                android:id="@+id/tatasky_eightBtn"
                android:layout_width="0dip"
                android:layout_height="fill_parent"
                android:layout_marginLeft="1dip"
                android:layout_weight="1"
                android:background="@drawable/radialgradientselector"
                android:text="8tuv"
                android:textColor="#FFFFFF"
                android:textSize="16dip"
                android:textStyle="bold" />

            <Button
                android:id="@+id/tatasky_nineBtn"
                android:layout_width="0dip"
                android:layout_height="fill_parent"
                android:layout_marginLeft="1dip"
                android:layout_weight="1"
                android:background="@drawable/radialgradientselector"
                android:text="9wxyz"
                android:textColor="#FFFFFF"
                android:textSize="16dip"
                android:textStyle="bold" />
        </TableRow>
    </TableLayout>

    <TableLayout
        android:id="@+id/tatasky_layer_no_pad_4"
        android:layout_width="fill_parent"
        android:layout_height="40dip"
        android:background="@drawable/gradient_menu" >

        <TableRow
            android:layout_width="fill_parent"
            android:layout_height="fill_parent"
            android:background="@android:color/transparent"
            android:gravity="center" >

            <Button
                android:id="@+id/tatasky_zeroBtn"
                android:layout_width="100dip"
                android:layout_height="fill_parent"
                android:layout_gravity="center_horizontal"
                android:background="@drawable/radialgradientselector"
                android:gravity="center"
                android:text="0-"
                android:textColor="#FFFFFF"
                android:textSize="16dip"
                android:textStyle="bold" />
        </TableRow>
    </TableLayout>

</LinearLayout>