package com.player.service;

public class ChannelInfo {
	private String Id;
	private String channelName;
	private String channelLogo;
	private String serviceCategory;
	private String serviceType;
	private String price;
	private String priceModel;
	private String subscribe;
	private String channelBouquetId;
	private String lock;
	private String like;
	private String isevent;
	private String timeStamp;
	private String description;
	
	public ChannelInfo(){
		
	}

	public ChannelInfo(String channelId,String channelName,String logo,String serviceCategory,String type, String priceModel, String price,
			String subscribe, String lock, String like, String isevent,String bouquetId,String time, String description){
		this.Id = channelId;
		this.serviceType = type;
		this.channelName = channelName;
		this.channelLogo = logo;
		this.price = price;
		this.priceModel = priceModel;
		this.subscribe = subscribe;
		this.lock = lock;
		this.like = like;
		this.serviceCategory = serviceCategory;
		this.isevent = isevent;
		this.channelBouquetId = bouquetId;
		this.timeStamp = time;
		this.description = description;
	}

	/**
	 * @return the id
	 */
	public String getId() {
		return Id;
	}

	/**
	 * @param id the id to set
	 */
	public void setId(String id) {
		Id = id;
	}

	/**
	 * @return the channelName
	 */
	public String getChannelName() {
		return channelName;
	}

	/**
	 * @param channelName the channelName to set
	 */
	public void setChannelName(String channelName) {
		this.channelName = channelName;
	}

	/**
	 * @return the channelLogo
	 */
	public String getChannelLogo() {
		return channelLogo;
	}

	/**
	 * @param channelLogo the channelLogo to set
	 */
	public void setChannelLogo(String channelLogo) {
		this.channelLogo = channelLogo;
	}

	/**
	 * @return the serviceCategory
	 */
	public String getServiceCategory() {
		return serviceCategory;
	}

	/**
	 * @param serviceCategory the serviceCategory to set
	 */
	public void setServiceCategory(String serviceCategory) {
		this.serviceCategory = serviceCategory;
	}

	/**
	 * @return the serviceType
	 */
	public String getServiceType() {
		return serviceType;
	}

	/**
	 * @param serviceType the serviceType to set
	 */
	public void setServiceType(String serviceType) {
		this.serviceType = serviceType;
	}

	/**
	 * @return the price
	 */
	public String getPrice() {
		return price;
	}

	/**
	 * @param price the price to set
	 */
	public void setPrice(String price) {
		this.price = price;
	}

	/**
	 * @return the priceModel
	 */
	public String getPriceModel() {
		return priceModel;
	}

	/**
	 * @param priceModel the priceModel to set
	 */
	public void setPriceModel(String priceModel) {
		this.priceModel = priceModel;
	}

	/**
	 * @return the subscribe
	 */
	public String getSubscribe() {
		return subscribe;
	}

	/**
	 * @param subscribe the subscribe to set
	 */
	public void setSubscribe(String subscribe) {
		this.subscribe = subscribe;
	}

	/**
	 * @return the lock
	 */
	public String getLock() {
		return lock;
	}

	/**
	 * @param lock the lock to set
	 */
	public void setLock(String lock) {
		this.lock = lock;
	}

	/**
	 * @return the like
	 */
	public String getLike() {
		return like;
	}

	/**
	 * @param like the like to set
	 */
	public void setLike(String like) {
		this.like = like;
	}

	/**
	 * @return the isevent
	 */
	public String getIsevent() {
		return isevent;
	}

	/**
	 * @param isevent the isevent to set
	 */
	public void setIsevent(String isevent) {
		this.isevent = isevent;
	}

	/**
	 * @return the timeStamp
	 */
	public String getTimeStamp() {
		return timeStamp;
	}

	/**
	 * @param timeStamp the timeStamp to set
	 */
	public void setTimeStamp(String timeStamp) {
		this.timeStamp = timeStamp;
	}
	
	/**
	 * @return the channelBouquetId
	 */
	public String getChannelBouquetId() {
		return channelBouquetId;
	}

	/**
	 * @param channelBouquetId the channelBouquetId to set
	 */
	public void setChannelBouquetId(String channelBouquetId) {
		this.channelBouquetId = channelBouquetId;
	}
	
	public String getDescription() {
		return description;
	}
	
	public void setDescription(String desc) {
		this.description= desc;
	}
	
	
}
